{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@blogUi/*": ["src/components/Blog/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
